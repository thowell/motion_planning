<robot name = "flip_5link">
  <material name = "link-color">
	<color rgba = "0.5 0.5 0.5 1" />
  </material>
  <material name = "Hebi-X-color">
    <color rgba = "1 0.27 0 1" />
  </material>
  <material name = "Hebi-X-color2">
    <color rgba = "0.9 0.3 0 1" />
  </material>

	<link name="world_link">
	  </link>

	<link name="x_link">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<mass value="1e-16" />
			<inertia ixx="1e-16" ixy="0" ixz="0" iyy="1e-16" iyz="0" izz="1e-16" />
		</inertial>
	</link>

	<link name="z_link">
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<mass value="1e-16" />
			<inertia ixx="1e-16" ixy="0" ixz="0" iyy="1e-16" iyz="0" izz="1e-16" />
		</inertial>
	</link>

	<link name = "base">
		<inertial>
			<origin xyz = "0 0 0" />
			<mass value = "1.46" />
			<inertia ixx = "0.0" iyy = "0.0029" izz = "0.0"
ixy = "0" ixz = "0" iyz = "0" />
		</inertial>
		<visual>
			<origin xyz = "0 0 0" />
			<geometry>
			  <box size = "0.1 0.4 0.3" />
			</geometry>
            <material name = "link-color"/>
		</visual>
        <!-- Left hip motor -->
        <visual>
          <origin xyz = "0 0.2225 -0.1125" />
          <geometry>
            <box size = "0.11 0.045 0.075" />
          </geometry>
          <material name = "Hebi-X-color"/>
        </visual>
        <!-- Right hip motor -->
        <visual>
          <origin xyz = "0 -0.2225 -0.1125" />
          <geometry>
            <box size = "0.11 0.045 0.075" />
          </geometry>
          <material name = "Hebi-X-color"/>
        </visual>
	</link>

	<link name = "left_thigh">
		<inertial>
			<origin xyz = "0.2176 0 0" />
			<mass value = "0.8112" />
			<inertia ixx = "0.0" iyy = "0.00709" izz = "0.0"
	ixy = "0" ixz = "0" iyz = "0" />
		</inertial>
		<visual>
		  <!-- <origin rpy="0 1.57075 0" xyz = "0.138  0.265 -0.1125" /> -->
          <origin rpy = "0 1.57075 0" xyz = "0.138 0 0"/>
			<geometry>
			  <cylinder radius = "0.02" length = "0.2755"  />
			</geometry>
			<material name = "link-color"/>
		</visual>
        <!-- Left knee motor -->
        <visual>
          <!-- <origin rpy = "-1.57075 0 0" xyz = "0.2755 -0.0225 0" /> -->
          <origin xyz = "0.2405 0 -0.04225"/>
          <geometry>
            <box size = "0.11 0.075 0.045" />
          </geometry>
          <material name = "Hebi-X-color2"/>
        </visual>
	</link>

	<joint name="virtual_x" type="prismatic">
    <parent link="world_link" />
    <child link="x_link" />
    <origin xyz="0 0 0" />
    <axis xyz="1 0 0" />
		<limit velocity="1e16" effort="1e16"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

	<joint name="virtual_z" type="prismatic">
    <parent link="x_link" />
    <child link="z_link" />
    <origin xyz="0 0 0" />
    <axis xyz="0 0 1" />
		<limit velocity="1e16" effort="1e16"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

	<joint name="virtual_rot" type="revolute">
    <parent link="z_link" />
    <child link="base" />
    <origin xyz="0 0 0" />
    <axis xyz="0 1 0" />
		<limit velocity="1e16" effort="1e16"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

	<joint name = "left_hip" type = "revolute">
		<parent link = "base" />
		<child link = "left_thigh" />
	    <origin rpy = "-1.57075 0  0" xyz = "0. 0.265 -0.1125" />
		<axis xyz = "0 0 1" />
        <limit lower="0.0" upper="0.548" velocity="0.5" effort="30.0"/>
	</joint>

	<link name = "left_shank">
		<inertial>
			<origin xyz = "0.1445 0 0" />
			<mass value = "0.3037" />
			<inertia ixx = "0.0" iyy = "0.00398" izz = "0.0"
	ixy = "0" ixz = "0" iyz = "0" />
		</inertial>
		<visual>
          <origin rpy = "0 1.57075 0" xyz = "0.144 0 0"/>
			<geometry>
			  <cylinder radius = "0.02" length = "0.288"/> <!-- total 0.308 (cylinder + phere) -->
			</geometry>
			<material name = "link-color"/>
		</visual>
        <visual>
          <origin rpy = "0 0 0" xyz = "0.288 0 0"/>
			<geometry>
			  <GeometryTypes.Sphere radius = "0.02"/>
			</geometry>
            <!-- <material name = "link-color"/> -->
            <material name = "Hebi-X-color"/>
        </visual>
	</link>

	<joint name = "left_knee" type = "revolute">
		<parent link = "left_thigh" />
		<child link = "left_shank" />
	    <origin rpy = "3.141592 0 0" xyz = "0.2755 0 -0.045" />
		<axis xyz = "0 0 -1" />
        <limit lower="0.0" upper="0.548" velocity="0.5" effort="30.0"/>
	</joint>

    <!-- right leg -->
	<link name = "right_thigh">
		<inertial>
			<origin xyz = "0.2176 0 0" />
			<mass value = "0.8112" />
			<inertia ixx = "0.0" iyy = "0.00709" izz = "0.0"
	ixy = "0" ixz = "0" iyz = "0" />
		</inertial>
		<visual>
          <origin rpy = "0 1.57075 0" xyz = "0.138 0 0"/>
			<geometry>
			  <cylinder radius = "0.02" length = "0.2755"  />
			</geometry>
			<material name = "link-color"/>
		</visual>
        <!-- right knee motor -->
        <visual>
          <origin xyz = "0.2405 0 -0.04225"/>
          <geometry>
            <box size = "0.11 0.075 0.045" />
          </geometry>
          <material name = "Hebi-X-color2"/>
        </visual>
	</link>

	<joint name = "right_hip" type = "revolute">
		<parent link = "base" />
		<child link = "right_thigh" />
	    <origin rpy = "1.57075 0  0" xyz = "0. -0.265 -0.1125" />
		<axis xyz = "0 0 -1" />
        <limit lower="0.0" upper="0.548" velocity="0.5" effort="30.0"/>
	</joint>

	<link name = "right_shank">
		<inertial>
			<origin xyz = "0.1445 0 0" />
			<mass value = "0.3037" />
			<inertia ixx = "0.0" iyy = "0.00398" izz = "0.0"
	ixy = "0" ixz = "0" iyz = "0" />
		</inertial>
		<visual>
          <origin rpy = "0 1.57075 0" xyz = "0.144 0 0"/>
			<geometry>
			  <cylinder radius = "0.02" length = "0.288"/> <!-- total 0.308 (cylinder + phere) -->
			</geometry>
			<material name = "link-color"/>
		</visual>
        <visual>
          <origin rpy = "0 0 0" xyz = "0.288 0 0"/>
			<geometry>
			  <GeometryTypes.Sphere radius = "0.02"/>
			</geometry>
            <!-- <material name = "link-color"/> -->
            <material name = "Hebi-X-color"/>
        </visual>
	</link>

	<joint name = "right_knee" type = "revolute">
		<parent link = "right_thigh" />
		<child link = "right_shank" />
	    <origin rpy = "3.141592 0 0" xyz = "0.2755 0 -0.045" />
		<axis xyz = "0 0 1" />
        <limit lower="0.0" upper="0.548" velocity="0.5" effort="30.0"/>
	</joint>
</robot>
